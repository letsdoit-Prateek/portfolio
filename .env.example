# GitHub API Token (for fetching repositories data)
GITHUB_TOKEN=auth-token

# OpenAI API Key (for AI chat functionality)
OPENAI_API_KEY=api-key

# Firebase Configuration (for Google Analytics)
NEXT_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
NEXT_PUBLIC_FIREBASE_APP_ID=your-app-id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Next.js Environment Variables
NEXT_PUBLIC_APP_URL=https://your-app-url.azurewebsites.net
NODE_ENV=production
PORT=3000

# Analytics (if using Vercel Analytics)
# VERCEL_ANALYTICS_ID=your_analytics_id

# Other environment variables used by your app
# Add your specific environment variables here
